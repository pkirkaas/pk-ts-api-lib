{
  "name": "pk-ts-api-lib",
  "author": "Paul Kirkaas",
  "license": "UNLICENSED",
  "description": "TS/JS Utilities for NodeJS Express APIs, including common & Node JS functions",
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./tsconfig.cjs.json": "./tsconfig.cjs.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json",
    "devlink": "npm link pk-ts-node-lib && npm link",
    "clean": "rm -rf ./dist ./node_modules package-lock.json",
    "cleani": "npm run clean && npm i",
    "monitor": "nodemon --exec \"npm run build\" --ext js,jsx,ts,tsx,sass,css --delay 5 --watch ./src",
    "null": "nothing"
  },
  "dependencies": {
    "bcrypt": "*",
    "body-parser": "*",
    "compression": "*",
    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "*",
    "cors": "*",
    "evp_bytestokey": "*",
    "express": "*",
    "express-async-errors": "*",
    "express-list-endpoints": "^6.0.0",
    "jsonwebtoken": "*",
    "kill-port": "*",
    "lodash": "^4.17.21",
    "pk-ts-node-lib": "github:pkirkaas/pk-ts-node-lib#working",
    "reflect-metadata": "*",
    "rxjs": "*",
    "serialize-error": "*"
  },
  "alt-lib-gits": [
    {
      "pk-ts-node-lib": "git+https://github.com/pkirkaas/pk-ts-node-lib.git#working",
      "no": "nothing"
    },
    {
      "pk-ts-node-lib": "github:pkirkaas/pk-ts-node-lib#working",
      "no": "nothing"
    },
    {
      "pk-ts-node-lib": "git+https://github.com/pkirkaas/pk-ts-node-lib.git#working",
      "no": "nothing"
    },
    {
      "no": "nothing"
    },
    {
      "no": "nothing"
    }
  ]
}
